{
    "name": "sobolan/flpoker",
    "type": "project",
    "description": "The site dedicated to FileList.ro's PokerStars poker club",
    "authors": [
        {
            "name": "Radu Murzea",
            "email": "radu.murzea@gmail.com",
            "homepage": "http://radumurzea.net/",
            "role": "Project Manager"
        }
